<!DOCTYPE html>
<html lang="pt">
<head>
  <meta charset="UTF-8">
  <title>My CRUD in NodeJS</title>
</head>
<body>

<h2>Registros<h2>
    <table border="1">
        <thead>
            <tr>
                <td>Nome</td>
                <td>Preço</td>
                <td>Ação</td>
            </tr>
        </thead>
        <tbody>
        <% data.forEach(function(details) { %>
            <tr>
                <td><%= details.name %></td>
                <td><%= details.price %></td>
                <td><a href="/products/<%= details._id %>/update">Editar</a> . <a href="/products/<%= details._id %>/delete">Deletar</a> </td>
            </tr>
            <% }) %>
        </tbody>
        <button><a href="/">Novo</a></button>

</body>
</html>
